<idea-plugin version="2">
	<extensions defaultExtensionNs="org.eclipse.xtext.idea">
		<resourceFactory type="inheritancetest3language"
						 class="org.eclipse.xtext.resource.IResourceFactory"
						 factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"/>
		<resourceServiceProvider uriExtension="inheritancetest3language"
								 class="org.eclipse.xtext.idea.resource.IResourceIdeaServiceProvider"
								 factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"/>

		<lang.setup language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
					implementationClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageIdeaSetup"/>
	</extensions>

	<extensions defaultExtensionNs="com.intellij">
		<psi.treeChangePreprocessor implementation="org.eclipse.xtext.grammarinheritance.idea.lang.psi.InheritanceTest3LanguageCodeBlockModificationListener"/>

		<fileTypeFactory implementation="org.eclipse.xtext.grammarinheritance.idea.lang.InheritanceTest3LanguageFileTypeFactory"/>
		<stubElementTypeHolder class="org.eclipse.xtext.grammarinheritance.idea.lang.InheritanceTest3LanguageElementTypeProvider"/>
		<lang.ast.factory language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
								factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
								implementationClass="org.eclipse.xtext.idea.lang.BaseXtextASTFactory"/>
		<lang.parserDefinition language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
								factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
								implementationClass="org.eclipse.xtext.grammarinheritance.idea.lang.parser.InheritanceTest3LanguageParserDefinition"/>
		<lang.findUsagesProvider language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
								factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
								implementationClass="org.eclipse.xtext.idea.findusages.BaseXtextFindUsageProvider"/>
		<lang.refactoringSupport language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
								factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
								implementationClass="org.eclipse.xtext.idea.refactoring.BaseXtextRefactoringSupportProvider"/>
		<lang.namesValidator language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
								factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
								implementationClass="com.intellij.lang.refactoring.NamesValidator"/>
      	<lang.syntaxHighlighterFactory key="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language" implementationClass="org.eclipse.xtext.grammarinheritance.idea.highlighting.InheritanceTest3LanguageSyntaxHighlighterFactory" />
		<lang.braceMatcher language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
								factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
								implementationClass="com.intellij.lang.PairedBraceMatcher"/>
      	<annotator language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
      							factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
      							implementationClass="org.eclipse.xtext.idea.annotation.IssueAnnotator"/>
      	<completion.contributor language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language" implementationClass="org.eclipse.xtext.grammarinheritance.idea.completion.InheritanceTest3LanguageCompletionContributor"/>
      	<elementDescriptionProvider implementation="org.eclipse.xtext.grammarinheritance.idea.lang.psi.InheritanceTest3LanguageElementDescriptionProvider" order="first"/>
      	<pom.declarationSearcher implementation="org.eclipse.xtext.grammarinheritance.idea.lang.pom.InheritanceTest3LanguagePomDeclarationSearcher"/>

      	<lang.psiStructureViewFactory language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
      							factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
      							implementationClass="com.intellij.lang.PsiStructureViewFactory"/>
		<facetType implementation="org.eclipse.xtext.grammarinheritance.idea.facet.InheritanceTest3LanguageFacetType"/>
		<colorSettingsPage implementation="org.eclipse.xtext.grammarinheritance.idea.highlighting.InheritanceTest3LanguageColorSettingsPage"/>
		<highlightVisitor implementation="org.eclipse.xtext.grammarinheritance.idea.highlighting.InheritanceTest3LanguageSemanticHighlightVisitor"/>

		<lang.formatter language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
								factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
								implementationClass="com.intellij.formatting.FormattingModelBuilder"/>
		<lang.commenter language="org.eclipse.xtext.grammarinheritance.InheritanceTest3Language"
								factoryClass="org.eclipse.xtext.grammarinheritance.idea.InheritanceTest3LanguageExtensionFactory"
								implementationClass="com.intellij.lang.CodeDocumentationAwareCommenter"/>
	</extensions>
</idea-plugin>
