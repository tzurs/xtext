<idea-plugin version="2">
	<extensions defaultExtensionNs="org.eclipse.xtext.idea">
		<resourceFactory type="fragmenttestlanguageex"
						 class="org.eclipse.xtext.resource.IResourceFactory"
						 factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"/>
		<resourceServiceProvider uriExtension="fragmenttestlanguageex"
								 class="org.eclipse.xtext.idea.resource.IResourceIdeaServiceProvider"
								 factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"/>

		<lang.setup language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
					implementationClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExIdeaSetup"/>
	</extensions>

	<extensions defaultExtensionNs="com.intellij">
		<psi.treeChangePreprocessor implementation="org.eclipse.xtext.parser.fragments.idea.lang.psi.FragmentTestLanguageExCodeBlockModificationListener"/>

		<fileTypeFactory implementation="org.eclipse.xtext.parser.fragments.idea.lang.FragmentTestLanguageExFileTypeFactory"/>
		<stubElementTypeHolder class="org.eclipse.xtext.parser.fragments.idea.lang.FragmentTestLanguageExElementTypeProvider"/>
		<lang.ast.factory language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
								factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
								implementationClass="org.eclipse.xtext.idea.lang.BaseXtextASTFactory"/>
		<lang.parserDefinition language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
								factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
								implementationClass="org.eclipse.xtext.parser.fragments.idea.lang.parser.FragmentTestLanguageExParserDefinition"/>
		<lang.findUsagesProvider language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
								factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
								implementationClass="org.eclipse.xtext.idea.findusages.BaseXtextFindUsageProvider"/>
		<lang.refactoringSupport language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
								factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
								implementationClass="org.eclipse.xtext.idea.refactoring.BaseXtextRefactoringSupportProvider"/>
		<lang.namesValidator language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
								factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
								implementationClass="com.intellij.lang.refactoring.NamesValidator"/>
      	<lang.syntaxHighlighterFactory key="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx" implementationClass="org.eclipse.xtext.parser.fragments.idea.highlighting.FragmentTestLanguageExSyntaxHighlighterFactory" />
		<lang.braceMatcher language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
								factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
								implementationClass="com.intellij.lang.PairedBraceMatcher"/>
      	<annotator language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
      							factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
      							implementationClass="org.eclipse.xtext.idea.annotation.IssueAnnotator"/>
      	<completion.contributor language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx" implementationClass="org.eclipse.xtext.parser.fragments.idea.completion.FragmentTestLanguageExCompletionContributor"/>
      	<elementDescriptionProvider implementation="org.eclipse.xtext.parser.fragments.idea.lang.psi.FragmentTestLanguageExElementDescriptionProvider" order="first"/>
      	<pom.declarationSearcher implementation="org.eclipse.xtext.parser.fragments.idea.lang.pom.FragmentTestLanguageExPomDeclarationSearcher"/>

      	<lang.psiStructureViewFactory language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
      							factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
      							implementationClass="com.intellij.lang.PsiStructureViewFactory"/>
		<facetType implementation="org.eclipse.xtext.parser.fragments.idea.facet.FragmentTestLanguageExFacetType"/>
		<colorSettingsPage implementation="org.eclipse.xtext.parser.fragments.idea.highlighting.FragmentTestLanguageExColorSettingsPage"/>
		<highlightVisitor implementation="org.eclipse.xtext.parser.fragments.idea.highlighting.FragmentTestLanguageExSemanticHighlightVisitor"/>

		<lang.formatter language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
								factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
								implementationClass="com.intellij.formatting.FormattingModelBuilder"/>
		<lang.commenter language="org.eclipse.xtext.parser.fragments.FragmentTestLanguageEx"
								factoryClass="org.eclipse.xtext.parser.fragments.idea.FragmentTestLanguageExExtensionFactory"
								implementationClass="com.intellij.lang.CodeDocumentationAwareCommenter"/>
	</extensions>
</idea-plugin>
